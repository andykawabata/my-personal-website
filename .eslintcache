th = 50;\n    p.lineY = 75\n    p.lineX = p.canvasX - p.lineLength;\n    p.circles = [];\n    p.counter = 0;\n\n    p.setup = () => {\n      p.createCanvas(p.canvasX, p.canvasY);\n      p.stroke(255);\n    }\n\n    p.draw = () => {\n      if(p.counter === 0){\n        let myCircle = new Circle(p.startX, p.random(p.startMinY, p.startMaxY), p.radius);\n        p.circles.push(myCircle);\n      }\n      p.counter = p.increment(p.counter, 0);\n      p.background(135, 206, 235);\n      p.line(p.canvasX, p.lineY, p.lineX, p.lineY);\n      p.fill(90, 77, 65)\n      p.stroke(0);\n      p.cliff();\n      //image(img, 0, 350, 300, 50)\n      for(let i=0; i<p.circles.length; i++){\n        var gravity = p.createVector(0, 0.2);\n        p.circles[i].applyForce(gravity);\n        p.circles[i].checkEdges();\n        p.circles[i].update();\n        p.circles[i].show();\n        if(p.circles[i].pos.y >= 350)\n          p.circles.splice(i, 1);\n      }\n    }\n\n    p.increment = function(counter, range) {\n      if(counter > range)\n        return 0;\n      return counter + 1;\n    }\n\n    p.cliff = function(){\n      let numRocks = 8;\n      let shortenBy = 8\n      let rockHeight = (p.canvasY-p.lineY)/numRocks;\n      let recs = [];\n      for(let i = 0; i < numRocks; i++){\n        let x = p.canvasX - p.lineLength + (shortenBy*i);\n        let y = p.lineY + (rockHeight * i);\n        let w = p.lineLength - (shortenBy * i);\n        let h = rockHeight;\n        let rec = p.rect(x,y,w,h);\n        recs.push(rec)\n      }\n      return recs;\n    }\n\n    class Circle{\n\n      constructor(x, y, radius){\n        this.pos = p.createVector(x, y);\n        this.vel = p.createVector(-.5, 0);\n        this.acc = p.createVector(0,0);\n        this.r = radius;\n      }\n    \n      applyForce(force){\n        this.acc.add(force)\n      }\n    \n      checkEdges(){\n    \n        //if on line\n        if(this.pos.y >= p.lineY - this.r && this.pos.x >= p.lineX){\n          \n          this.vel.y *= -.95;\n          this.acc.set(0,0);\n        }\n      }\n    \n      update(){\n        this.vel.add(this.acc);\n        this.pos.add(this.vel);\n        this.acc.set(0,0);\n      }\n    \n      show(){\n        p.stroke(255);\n        p.strokeWeight(0);\n        p.fill(20,20, 100);\n        p.ellipse(this.pos.x, this.pos.y, this.r*2)\n      }\n    \n    \n    }\n\n  }\n\n  \n\n  return (\n    <div className=\"waterfall\" ref={myRef}>\n      \n    </div>\n  );\n}\n\nexport default Waterfall;\n","C:\\Users\\andyk\\Desktop\\CodingProjects\\my-website\\src\\components\\projects\\Projects.js",["52","53","54","55","56"],"import React, { useState } from 'react';\r\nimport divider from './fern.svg'\r\nimport './Projects.css'\r\nimport Project from './Project'\r\nimport { useEffect } from 'react';\r\nimport data from './data/Data.js'\r\n\r\nfunction Projects(){\r\n    \r\n    const minWidth = 800;\r\n\r\n    const [isSmallScreen, setIsSmallScreen] = useState(null);\r\n\r\n    useEffect(() => {\r\n        setIsSmallScreen(checkIfSmallScreen());\r\n        window.addEventListener(\"resize\", handleResize);\r\n    },[])\r\n\r\n    const handleResize = (e) => {\r\n        setIsSmallScreen(checkIfSmallScreen());\r\n    };\r\n\r\n    function checkIfSmallScreen(){\r\n        return window.innerWidth < minWidth ? true : false;\r\n\r\n    }\r\n\r\n    return (\r\n        <div className=\"outer-projects-container\">\\\r\n\r\n\r\n            {/*/////////  PERSONAL HEADER ///////////*/}\r\n            <div className=\"projects-header personal\">\r\n                <div className=\"project-header-inner\">\r\n                    <h1>Personal Projects</h1>\r\n                    <p>\r\n                        Applications and tools I've built in my free time to solve problems and experiment with new technologies.\r\n                    </p>\r\n                    <img className=\"divider\" src={divider}/>\r\n                </div>\r\n            </div>\r\n\r\n            {/*///